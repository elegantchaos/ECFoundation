// --------------------------------------------------------------------------
// Build Options
// --------------------------------------------------------------------------

PRECOMPS_INCLUDE_HEADERS_FROM_BUILT_PRODUCTS_DIR = NO

// --------------------------------------------------------------------------
// Deployment
// --------------------------------------------------------------------------

INSTALL_PATH = $(HOME)/Applications

// --------------------------------------------------------------------------
// Architectures
// --------------------------------------------------------------------------

ARCHS = $(ARCHS_STANDARD_32_64_BIT)
SDKROOT = macosx10.6


// --------------------------------------------------------------------------
// Code Generation
// --------------------------------------------------------------------------

GCC_VERSION = com.apple.compilers.llvm.clang.1_0
GCC_MODEL_TUNING = G5
